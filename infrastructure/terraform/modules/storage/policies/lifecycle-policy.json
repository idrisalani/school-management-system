{
    "Rules": [
      {
        "ID": "StandardDocumentsRule",
        "Status": "Enabled",
        "Filter": {
          "Prefix": "documents/"
        },
        "Transitions": [
          {
            "Days": 30,
            "StorageClass": "STANDARD_IA"
          },
          {
            "Days": 90,
            "StorageClass": "GLACIER"
          }
        ],
        "Expiration": {
          "Days": 365
        },
        "NoncurrentVersionTransitions": [
          {
            "NoncurrentDays": 30,
            "StorageClass": "STANDARD_IA"
          },
          {
            "NoncurrentDays": 60,
            "StorageClass": "GLACIER"
          }
        ],
        "NoncurrentVersionExpiration": {
          "NoncurrentDays": 90
        },
        "AbortIncompleteMultipartUpload": {
          "DaysAfterInitiation": 7
        }
      },
      {
        "ID": "StudentAssignmentsRule",
        "Status": "Enabled",
        "Filter": {
          "Prefix": "assignments/"
        },
        "Transitions": [
          {
            "Days": 60,
            "StorageClass": "STANDARD_IA"
          },
          {
            "Days": 180,
            "StorageClass": "GLACIER"
          }
        ],
        "Expiration": {
          "Days": 730
        }
      },
      {
        "ID": "TemporaryUploadsRule",
        "Status": "Enabled",
        "Filter": {
          "Prefix": "temp/"
        },
        "Expiration": {
          "Days": 1
        }
      },
      {
        "ID": "LogsRetentionRule",
        "Status": "Enabled",
        "Filter": {
          "Prefix": "logs/"
        },
        "Transitions": [
          {
            "Days": 30,
            "StorageClass": "GLACIER"
          }
        ],
        "Expiration": {
          "Days": 90
        }
      },
      {
        "ID": "BackupsRetentionRule",
        "Status": "Enabled",
        "Filter": {
          "Prefix": "backups/"
        },
        "Transitions": [
          {
            "Days": 30,
            "StorageClass": "STANDARD_IA"
          },
          {
            "Days": 90,
            "StorageClass": "GLACIER"
          }
        ],
        "NoncurrentVersionExpiration": {
          "NoncurrentDays": 180
        }
      },
      {
        "ID": "MediaFilesRule",
        "Status": "Enabled",
        "Filter": {
          "Prefix": "media/"
        },
        "Transitions": [
          {
            "Days": 45,
            "StorageClass": "STANDARD_IA"
          },
          {
            "Days": 120,
            "StorageClass": "GLACIER"
          }
        ],
        "NoncurrentVersionTransitions": [
          {
            "NoncurrentDays": 30,
            "StorageClass": "STANDARD_IA"
          }
        ]
      },
      {
        "ID": "DeleteMarkerCleanup",
        "Status": "Enabled",
        "Expiration": {
          "ExpiredObjectDeleteMarker": true
        }
      },
      {
        "ID": "IncompleteMultipartUploadsRule",
        "Status": "Enabled",
        "AbortIncompleteMultipartUpload": {
          "DaysAfterInitiation": 7
        }
      }
    ]
  }